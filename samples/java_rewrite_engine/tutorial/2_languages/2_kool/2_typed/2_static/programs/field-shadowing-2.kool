// Testing local variable shadowing a field

class C {
  int f;
  void C(int v) {
    f = v;
  }
  int get() { return f; }
}

class G {
  class(C) o;
  void G(class(C) o) {
    this.o = o;
  }
  int d() {
    int f = 9;
    return (o.get());
  }
}

class theMain {
  void theMain() {
    class(C) t = new C(1);
    class(G) y = new G(t);
    print(y.d(), "\n");
  }
}

